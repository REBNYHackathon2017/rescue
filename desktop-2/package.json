{
  "name": "Rescue",
  "version": "1.0.0",
  "description": "Rescue",
  "main": "lib/server.js",
  "repository": "",
  "author": "Rescue Team",
  "license": "",
  "scripts": {
    "dev": "NODE_PATH=./lib nodemon lib/server.js --exec babel-node",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "lint": "node_modules/.bin/esw webpack.config.* lib",
    "lint-watch": "yarn lint --watch",
    "test": "jest --watch",
    "verify-tests": "jest --coverage",
    "build-node": "babel lib -d build --copy-files",
    "start-prod": "NODE_ENV=production NODE_PATH=./build pm2 start build/server.js -i max --name appProd",
    "start-dev-docker": "NODE_ENV=development NODE_PATH=./build pm2-docker build/server.js",
    "start-prod-docker": "NODE_ENV=production NODE_PATH=./build pm2-docker build/server.js"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "enzyme": "^3.2.0",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "3.1.3",
    "jest": "^21.2.1",
    "nodemon": "1.12.5",
    "react-test-renderer": "^16.2.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^3.3.7",
    "chalk": "2.3.0",
    "css-loader": "^0.28.9",
    "ejs": "^2.5.7",
    "enzyme-adapter-react-16": "^1.1.0",
    "express": "^4.16.2",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^2.8.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-svg-loader": "^2.1.0",
    "redux": "3.7.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-thunk": "2.2.0",
    "webpack": "^3.10.0",
    "yarn-run-all": "3.1.1"
  }
}
